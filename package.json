    "husky": "^8.0.3",
    "jsdom": "^23.0.1",
    "path": "^0.12.7",
    "postcss": "^8.4.32",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.2.2",
